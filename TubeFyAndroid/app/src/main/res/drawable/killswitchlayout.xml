<?xml version="1.0" encoding="UTF-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android" >
    <item>
        <shape >
            <gradient
                    android:startColor="#77A1D3"
                    android:centerColor="#79CBCA"
                    android:endColor="#E684AE"
                    android:endY="80"
                    android:startY="0"
                    />
            <size android:width="24dp"
                android:height="24dp"/>
            <corners android:topLeftRadius="25dp" android:topRightRadius="25dp" android:bottomLeftRadius="25dp" android:bottomRightRadius="25dp"/>
        </shape>
    </item>

    <item
        android:bottom="2dp"
        android:left="2dp"
        android:right="2dp"
        android:top="2dp">
        <shape >
            <solid android:color="#313131" />
            <corners android:topLeftRadius="25dp" android:topRightRadius="25dp" android:bottomLeftRadius="25dp" android:bottomRightRadius="25dp"/>
        </shape>
    </item>

</layer-list>
